pipeline {
  agent any
  stages {
    stage("build") {
      steps {
        sh """
          docker build -t pytest-project .
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --rm pytest-project
        """
      }
    }
    stage('Unit Test') {
      steps {
        bat '.\\\\Scripts\\\\UnitTest.bat'
        junit '*.xml'
      }
    }
    stage('VI BUILD') {
      steps { 
        bat '.\\\\Scripts\\\\Build.bat'
      }
    }
  }
}